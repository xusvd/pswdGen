{
  "manifest_version": 3,
  "name": "Password Manager",
  "version": "1.0",
  "description": "A simple password manager extension",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup_size": {
      "width": 500,
      "height": 600
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}